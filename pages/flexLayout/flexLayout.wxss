/**index.wxss**/
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.check-detail {
  position: fixed;
  right: 5rpx;
  top: 5rpx;
}

.money-detail {
  height: 309rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.money-detail .img-money {
  width: 96rpx;
  height: 96rpx;
}

.text1 {
  color: #000;
}

.btn-withdraw {
  width: 600rpx;
  height: 88rpx;
  border-radius: 45rpx;
  background-color: #FE890B;
  margin-top: 150rpx;
}

.withdraw-rules {
  width: 609rpx;
  margin-top: 150rpx;
  display: flex;
  flex-direction: column;
}

.text2 {
  font-size: 16px;
  color: #888888;
  font-weight: bold;
}

.v-h-center{
  display: flex;
  justify-content: center;
  align-items: center;
}